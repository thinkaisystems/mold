            <div class="container custom-con">
      <h3><a href="/connection-list">Connection List</a></h3>
      <h1>Connection Detail</h1>
      <table>
        <tbody>
          <tr>
            <td>ConName</td>
            <td>:</td>
            <td><%= connection.ConName %></td>
          </tr>		 
		  <tr>
            <td>No Of Records</td>
            <td>:</td>
            <td><%= connection.NoOfRecords %></td>
          </tr>
		  <tr>
            <td>Status</td>
            <td>:</td>
            <td><%= connection.Status %></td>
          </tr>
          <tr>
            <td>ConType</td>
            <td>:</td>
            <td>
				<select class="selectpicker" data-style="btn-info" name="ConType" id="ConType" disabled>
					<optgroup label="Select connection">
						<option name="" value="0">--Select connection--</option>
						<option name="filelocation" value="1" <%= (connection.ConType == 1)?'selected':'' %>  >File Location</option>
						<option name="kafka" value="2" <%= (connection.ConType == 2)?'selected':'' %> >Kafka</option>
					</optgroup>
				</select>
			</td>
          </tr>
          <tr class="clsFileLocation">
            <td>File Location</td>
            <td>:</td>
            <td><%= connection.FileLocation %></td>
          </tr>
		  <tr class="clsKafka">
            <td>BrokerEndPoint</td>
            <td>:</td>
            <td><%= connection.BrokerEndPoint %></td>
          </tr>
		  <tr class="clsKafka">
            <td>TopicName</td>
            <td>:</td>
            <td><%= connection.TopicName %></td>
          </tr>		  		  
        </tbody>
      </table>
      <h3><a href="/connections/edit/<%= connection._id%>">EDIT</a></h3>
      <form action="/connections/delete/<%= connection._id%>" method="post">
        <button type="submit">DELETE</button>
      </form>
