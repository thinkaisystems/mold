    <script>
        $(document).ready(function() {

			if(<%= (connection.ConType == 1) %>)
			{
				$( ".clsKafka" ).hide();		
			}
			else if(<%= (connection.ConType == 2) %>)
			{
				$( ".clsFileLocation" ).hide();		
			}
		
